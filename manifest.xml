<!-- Garmin CPR Trainer App Manifest -->
<iq:manifest xmlns:iq="http://www.garmin.com/xml/connectiq" version="3">
    <iq:application entry="CPRTrainerApp" id="cpr-trainer-app" launcherIcon="@Drawables.LauncherIcon" minApiLevel="3.3.0" name="@Strings.AppName" type="watch-app" version="1.0.0">
        <iq:products>
            <!-- Supports most Garmin watches -->
            <iq:product id="fenix7"/>
            <iq:product id="fenix7s"/>
            <iq:product id="fenix7x"/>
            <iq:product id="fenix6"/>
            <iq:product id="fenix6s"/>
            <iq:product id="fenix6xpro"/>
            <iq:product id="fenix5"/>
            <iq:product id="fenix5s"/>
            <iq:product id="fenix5x"/>
            <iq:product id="venu"/>
            <iq:product id="venu2"/>
            <iq:product id="venu2plus"/>
            <iq:product id="venu2s"/>
            <iq:product id="vivoactive3"/>
            <iq:product id="vivoactive4"/>
            <iq:product id="vivoactive4s"/>
            <iq:product id="forerunner245"/>
            <iq:product id="forerunner255"/>
            <iq:product id="forerunner945"/>
            <iq:product id="forerunner955"/>
            <iq:product id="epix2"/>
            <iq:product id="marq"/>
        </iq:products>
        <iq:permissions>
            <iq:uses-permission id="Sensor"/>
            <iq:uses-permission id="Attention"/>
        </iq:permissions>
        <iq:languages>
            <iq:language>deu</iq:language>
            <iq:language>eng</iq:language>
        </iq:languages>
        <iq:barrels/>
    </iq:application>
</iq:manifest>
